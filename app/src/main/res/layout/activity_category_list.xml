<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CategoryList"
    android:background="@drawable/background_aawareness"
    >


    <TextView
        android:id="@+id/aaware_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text= "aaware"
        android:fontFamily="@font/megrim"
        android:textSize="30sp"
        android:gravity= "right"
        android:textColor="#FFDC00"
        android:textStyle="bold"
        android:background="@android:color/transparent"
        />
    <Button
        android:id="@+id/food"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="106dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="223dp"
        android:background="@drawable/roundedbutton"
        android:fontFamily="@font/megrim"
        android:gravity="bottom"
        android:text="Food"
        android:textAlignment="center"
        android:textSize="30dp" />

    <Button
        android:id="@+id/emergency"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="385dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="223dp"
        android:background="@drawable/roundedbutton"
        android:fontFamily="@font/megrim"
        android:gravity="bottom"
        android:text="Emergency"
        android:textAlignment="center"
        android:textSize="30dp" />

    <Button
        android:id="@+id/daily_routine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="278dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="223dp"
        android:background="@drawable/roundedbutton"
        android:fontFamily="@font/megrim"
        android:gravity="bottom"
        android:text="Daily Routine"
        android:textAlignment="center"
        android:textSize="30dp" />

    <Button
        android:id="@+id/health"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="195dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="223dp"
        android:background="@drawable/roundedbutton"
        android:fontFamily="@font/megrim"
        android:gravity="bottom"
        android:text="health"
        android:textAlignment="center"
        android:textSize="30dp" />

    <Button
        android:id="@+id/log_out"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="683dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="22dp"
        android:background="@drawable/roundedbutton"
        android:fontFamily="@font/megrim"
        android:gravity="bottom"
        android:text="log Out"
        android:textAlignment="center"
        android:textSize="30dp" />

    <Button
        android:id="@+id/more_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerInParent="true"
        android:layout_marginTop="488dp"
        android:layout_marginEnd="33dp"
        android:layout_marginRight="223dp"
        android:background="@drawable/roundedbutton"
        android:fontFamily="@font/megrim"
        android:gravity="bottom"
        android:text="More Info"
        android:textAlignment="center"
        android:textSize="30dp" />



</RelativeLayout>
